<!-- -*- mode: XML -*- -->
<launch>
  <arg name="racecar_version" default="racecar-v2" />
  <arg name="run_camera" default="false"/>


  <include file="$(find racecar)/launch/includes/$(arg racecar_version)-teleop.launch.xml">
    <arg name="racecar_version" value="$(arg racecar_version)" />
    <arg name="run_camera" value="$(arg run_camera)" />
  </include>

    <!-- Map server -->
    <node pkg="map_server" name="map_server" type="map_server" args="$(find racecar)/maps/subakio.yaml" >
        <param name="frame_id" value="map" />
    </node>
    <!-- AMCL used for localization -->
    <include file="$(find racecar)/launch/includes/racecar-v2/amcl.launch"/>

    <!-- Calls navigation stack -->
    <include file="$(find racecar)/launch/includes/racecar-v2/move_base.launch"/>

</launch>
